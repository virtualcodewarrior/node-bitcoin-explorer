{
  "name": "bitcoin-explorer",
  "version": "0.2.2",
  "description": "A block explorer clone built using BitcoinJS",
  "main": "./lib/bitcoin",
  "keywords": [
    "bitcoin",
    "realtime",
    "block",
    "transaction",
    "explorer",
    "monitoring",
    "viewer"
  ],

  "scripts": {
    "start": "node app.js"
  },

  "dependencies": {
    "bitcoinjs": ">=0.2.1",
    "ejs": ">=0.4.3",
    "lru-cache": ">=1.0.4",
    "winston": ">=0.3.4",
    "express": ">=4.0",
    "step": ">=0.0.4",
    "jsonrpc2": ">=0.1.0",
    "errorhandler": ">=1.0.1",
    "body-parser": ">=1.2.2",
    "method-override": ">=1.0.2"
  },

  "author": "Stefan Thomas <justmoon@members.fsf.org> (http://www.justmoon.net)",

  "repository": {
    "type": "git",
    "url": "git://github.com/bitcoinjs/node-bitcoin-explorer.git"
  }
}
